{
    "name": "zippy-devnav",
    "version": "0.0.2",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "clean": "rimraf dist",
        "build": "pnpm run clean && tsc -p tsconfig.lib.json && cpx \"src/style/*.css\" dist/style",
        "type-check": "tsc --noEmit",
        "prepublishOnly": "pnpm run build",
        "dev": "vite",
        "build-vite": "tsc && vite build",
        "preview": "vite preview"
    },
    "devDependencies": {
        "cpx": "^1.5.0",
        "rimraf": "^6.0.1",
        "typescript": "~5.9.3",
        "vite": "npm:rolldown-vite@7.1.14"
    },
    "pnpm": {
        "overrides": {
            "vite": "npm:rolldown-vite@7.1.14"
        }
    },
    "files": [
        "dist",
        "README.md",
        "package.json"
    ]
}
